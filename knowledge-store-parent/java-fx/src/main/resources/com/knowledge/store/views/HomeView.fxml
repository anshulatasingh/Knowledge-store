<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TreeView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="737.0" prefWidth="1344.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.knowledge.store.controllers.HomeController">
   <children>
      <MenuBar fx:id="menuBar" opacity="0.88" prefHeight="36.0" prefWidth="808.0" styleClass="menu" stylesheets="@resources/knowledge-store.css" VBox.vgrow="ALWAYS">
        <menus>
          <Menu mnemonicParsing="false" text="File">
            <items>
              <MenuItem fx:id="closeMenuItem" mnemonicParsing="false" onAction="#onCloseAction" text="Close" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Edit">
            <items>
              <MenuItem mnemonicParsing="false" text="Delete" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Help">
            <items>
              <MenuItem mnemonicParsing="false" text="About" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
      <SplitPane fx:id="splitPane" dividerPositions="0.5" prefHeight="540.0" prefWidth="800.0" VBox.vgrow="ALWAYS">
         <items>
            <VBox prefHeight="200.0" prefWidth="100.0" style="-fx-border-width: 0;" styleClass="background" stylesheets="@resources/knowledge-store.css">
               <children>
                  <HBox fx:id="searchboxContainr" prefHeight="33.0" prefWidth="667.0" styleClass="statusbar" stylesheets="@resources/knowledge-store.css">
                     <padding>
                        <Insets bottom="2.0" left="2.0" right="2.0" top="2.0" />
                     </padding></HBox>
                  <TreeView fx:id="treeView" editable="true" prefHeight="657.0" prefWidth="667.0" styleClass="tree-cell" stylesheets="@resources/knowledge-store.css" VBox.vgrow="ALWAYS" />
               </children>
            </VBox>
            <TabPane fx:id="tabPane" rotateGraphic="true" />
         </items>
      </SplitPane>
      <HBox fx:id="statusBar" prefHeight="30.0" prefWidth="800.0" styleClass="statusbar" stylesheets="@resources/knowledge-store.css" />
   </children>
   <padding>
      <Insets bottom="2.0" left="2.0" right="2.0" top="2.0" />
   </padding>
</VBox>
